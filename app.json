{
  "expo": {
    "name": "TourOfHonor",
    "slug": "TourOfHonor",
    "version": "3.7.2",
    "orientation": "portrait",
    "icon": "./app/assets/appIcon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f4f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allows you to submit your memorial visits using the photos in your camera roll."
        }
      ],
      "expo-secure-store"
    ],
    "ios": {
      "buildNumber": "50",
      "bundleIdentifier": "com.ambitiousNerds.TourOfHonor",
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ambitiousNerds.TourOfHonor",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 28
    },
    "extra": {
      "eas": {
        "projectId": "58f65fc8-31fd-4e42-9b47-506ffc6c05bf"
      }
    }
  }
}
