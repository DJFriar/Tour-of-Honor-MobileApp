{
  "expo": {
    "name": "TourOfHonor",
    "slug": "TourOfHonor",
    "version": "3.1.1",
    "orientation": "portrait",
    "icon": "./app/assets/appIcon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f4f4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "35",
      "bundleIdentifier": "com.ambitiousNerds.TourOfHonor",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allows you to submit your memorial visits using the photos in your camera roll."
      },
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ambitiousNerds.TourOfHonor",
      "permissions": ["READ_EXTERNAL_STORAGE"],
      "versionCode": 13
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}